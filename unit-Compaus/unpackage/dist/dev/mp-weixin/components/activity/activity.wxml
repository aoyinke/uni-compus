<view class="data-v-4ea9ceb8"><block wx:for="{{activityInfo}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><block class="data-v-4ea9ceb8"><view class="groupNotification data-v-4ea9ceb8"><user-top-bar vue-id="{{'08acfabc-1-'+idx}}" groupInfo="{{item.groupInfo}}" activityStartTime="{{item.activityStartTime}}" activityId="1" class="data-v-4ea9ceb8" bind:__l="__l"></user-top-bar><go-detail vue-id="{{'08acfabc-2-'+idx}}" detailUrl="/pages/activityDetail/activityDetail" item="{{item}}" class="data-v-4ea9ceb8" bind:__l="__l" vue-slots="{{['default']}}"><view class="mainBar data-v-4ea9ceb8"><block wx:if="{{item.title}}"><view class="activity-title data-v-4ea9ceb8"><text class="data-v-4ea9ceb8">{{item.title}}</text></view></block><block wx:if="{{item.img.length}}"><swiper class="swiper data-v-4ea9ceb8" indicator-dots="{{true}}"><block wx:for="{{item.img}}" wx:for-item="img" wx:for-index="id" wx:key="id"><swiper-item class="data-v-4ea9ceb8"><view class="swiper-item data-v-4ea9ceb8"><image src="{{img}}" mode class="data-v-4ea9ceb8"></image></view></swiper-item></block></swiper></block><block wx:if="{{item.description}}"><view class="description text-line-4 data-v-4ea9ceb8"><text class="data-v-4ea9ceb8">{{item.description}}</text></view></block></view></go-detail><view class="footer data-v-4ea9ceb8"><mutation vue-id="{{'08acfabc-3-'+idx}}" class="data-v-4ea9ceb8" bind:__l="__l"></mutation><view class="commentDetail data-v-4ea9ceb8"><view class="activityInfo data-v-4ea9ceb8"><text class="activityInfo-left data-v-4ea9ceb8">{{item.hotNum+"热度"}}</text><text class="activityInfo-right data-v-4ea9ceb8">{{item.commentNum+"评论"}}</text></view><block wx:for="{{item.commentDetail}}" wx:for-item="comment" wx:for-index="index" wx:key="index"><block class="data-v-4ea9ceb8"><view class="commentDetail-P data-v-4ea9ceb8"><text class="commentDetail-P-left data-v-4ea9ceb8">{{comment.commentor+':'}}</text><text class="commentDetail-P-right data-v-4ea9ceb8">{{comment.commentContent}}</text></view></block></block></view></view></view></block></block></view>